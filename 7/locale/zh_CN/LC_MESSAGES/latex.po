msgid ""
"# inside conf.py\n"
"latex_engine = 'xelatex'\n"
"latex_elements = {\n"
"    'passoptionstopackages': r'''\n"
"\\PassOptionsToPackage{svgnames}{xcolor}\n"
"''',\n"
"    'fontpkg': r'''\n"
"\\setmainfont{DejaVu Serif}\n"
"\\setsansfont{DejaVu Sans}\n"
"\\setmonofont{DejaVu Sans Mono}\n"
