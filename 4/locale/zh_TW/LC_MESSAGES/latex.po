"# inside conf.py\n"
"latex_engine = 'xelatex'\n"
"latex_elements = {\n"
"    'fontpkg': r'''\n"
"\\setmainfont{DejaVu Serif}\n"
"\\setsansfont{DejaVu Sans}\n"
"\\setmonofont{DejaVu Sans Mono}\n"
