#: ../../../usage/extensions/apidoc.rst:47
msgid ""
"A list or sequence of dictionaries describing modules to document. If a "
"value is left unspecified in any dictionary, the general configuration value "
"is used as the default."
msgstr ""

#: ../../../usage/extensions/apidoc.rst:51
msgid "For example:"
msgstr ""

#: ../../../usage/extensions/apidoc.rst:53
msgid ""
"apidoc_modules = [\n"
"    {'path': 'path/to/module', 'destination': 'source/'},\n"
"    {\n"
"        'path': 'path/to/another_module',\n"
"        'destination': 'source/',\n"
"        'exclude_patterns': ['**/test*'],\n"
"        'max_depth': 4,\n"
"        'follow_links': False,\n"
"        'separate_modules': False,\n"
"        'include_private': False,\n"
"        'no_headings': False,\n"
"        'module_first': False,\n"
"        'implicit_namespaces': False,\n"
"        'automodule_options': {\n"
"            'members', 'show-inheritance', 'undoc-members'\n"
"        },\n"
"    },\n"
"]"
